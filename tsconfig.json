{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "paths": {
      "@spartan-ng/helm/button": [
        "./libs/ui/button/src/index.ts"
      ],
      "@spartan-ng/helm/utils": [
        "./libs/ui/utils/src/index.ts"
      ],
      "@spartan-ng/helm/sonner": [
        "./libs/ui/sonner/src/index.ts"
      ],
      "@spartan-ng/helm/icon": [
        "./libs/ui/icon/src/index.ts"
      ],
      "@spartan-ng/helm/select": [
        "./libs/ui/select/src/index.ts"
      ],
      "@spartan-ng/helm/switch": [
        "./libs/ui/switch/src/index.ts"
      ],
      "@spartan-ng/helm/label": [
        "./libs/ui/label/src/index.ts"
      ],
      "@spartan-ng/helm/dropdown-menu": [
        "./libs/ui/dropdown-menu/src/index.ts"
      ],
      "@spartan-ng/helm/field": [
        "./libs/ui/field/src/index.ts"
      ],
      "@spartan-ng/helm/separator": [
        "./libs/ui/separator/src/index.ts"
      ],
      "@spartan-ng/helm/checkbox": [
        "./libs/ui/checkbox/src/index.ts"
      ],
      "@spartan-ng/helm/input": [
        "./libs/ui/input/src/index.ts"
      ],
      "@spartan-ng/helm/textarea": [
        "./libs/ui/textarea/src/index.ts"
      ],
      "@spartan-ng/helm/form-field": [
        "./libs/ui/form-field/src/index.ts"
      ],
      "@spartan-ng/helm/dialog": [
        "./libs/ui/dialog/src/index.ts"
      ],
      "@spartan-ng/helm/card": [
        "./libs/ui/card/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
